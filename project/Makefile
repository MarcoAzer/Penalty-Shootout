# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  project_config = debug
endif
ifeq ($(config),release)
  project_config = release
endif

PROJECTS := project

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

project:
ifneq (,$(project_config))
	@echo "==== Building project ($(project_config)) ===="
	@${MAKE} --no-print-directory -C build -f Makefile config=$(project_config)
endif

clean:
	@${MAKE} --no-print-directory -C build -f Makefile clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   project"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"